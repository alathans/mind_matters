<div class="page-header">
  <h1>
    Insurers
    <a href="/insurers/new" class="btn btn-lg btn-success">New Insurer</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Insurer name</th>
        <th>Actions</th>
      </tr>

      <% @insurers.each do |insurer| %>
      <tr>
        <td><%= insurer.insurer_name %></td>
        <td>
          <a href="/insurers/<%= insurer.id %>" class="btn btn-primary">Show</a>
          <a href="/insurers/<%= insurer.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_insurer/<%= insurer.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
