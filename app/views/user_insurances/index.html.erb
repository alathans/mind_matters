<div class="page-header">
  <h1>
    User insurances
    <a href="/user_insurances/new" class="btn btn-lg btn-success">New User insurance</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Uninsured</th>
        <th>Insurer</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @user_insurances.each do |user_insurance| %>
      <tr>
        <td><%= user_insurance.uninsured %></td>
        <td><%= user_insurance.insurer_id %></td>
        <td><%= user_insurance.user_id %></td>
        <td>
          <a href="/user_insurances/<%= user_insurance.id %>" class="btn btn-primary">Show</a>
          <a href="/user_insurances/<%= user_insurance.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_user_insurance/<%= user_insurance.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
