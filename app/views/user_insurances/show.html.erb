<div class="page-header">
  <h3>User Insurance #<%= @user_insurance.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Uninsured</dt>
      <dd><%= @user_insurance.uninsured %></dd>

      <dt>Insurer</dt>
      <dd><%= @user_insurance.insurer_id %></dd>

      <dt>User</dt>
      <dd><%= @user_insurance.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/user_insurances" class="btn btn-primary">
        Back
      </a>
      <a href="/user_insurances/<%= @user_insurance.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_user_insurance/<%= @user_insurance.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
