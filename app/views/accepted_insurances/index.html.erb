<div class="page-header">
  <h1>
    Accepted insurances
    <a href="/accepted_insurances/new" class="btn btn-lg btn-success">New Accepted insurance</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Insurer</th>
        <th>Therapist</th>
        <th>Actions</th>
      </tr>

      <% @accepted_insurances.each do |accepted_insurance| %>
      <tr>
        <td><%= accepted_insurance.insurer_id %></td>
        <td><%= accepted_insurance.therapist_id %></td>
        <td>
          <a href="/accepted_insurances/<%= accepted_insurance.id %>" class="btn btn-primary">Show</a>
          <a href="/accepted_insurances/<%= accepted_insurance.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_accepted_insurance/<%= accepted_insurance.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
