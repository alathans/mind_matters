<div class="page-header">
  <h1>
    Therapist availabilities
    <a href="/therapist_availabilities/new" class="btn btn-lg btn-success">New Therapist availability</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Time</th>
        <th>Therapist</th>
        <th>Actions</th>
      </tr>

      <% @therapist_availabilities.each do |therapist_availability| %>
      <tr>
        <td><%= therapist_availability.time_id %></td>
        <td><%= therapist_availability.therapist_id %></td>
        <td>
          <a href="/therapist_availabilities/<%= therapist_availability.id %>" class="btn btn-primary">Show</a>
          <a href="/therapist_availabilities/<%= therapist_availability.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_therapist_availability/<%= therapist_availability.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
