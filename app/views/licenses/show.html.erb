<div class="page-header">
  <h3>License #<%= @license.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>License number</dt>
      <dd><%= @license.license_number %></dd>

      <dt>State</dt>
      <dd><%= @license.state_id %></dd>

      <dt>Therapist</dt>
      <dd><%= @license.therapist_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/licenses" class="btn btn-primary">
        Back
      </a>
      <a href="/licenses/<%= @license.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_license/<%= @license.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
