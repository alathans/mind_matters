<div class="page-header">
  <h1>
    Licenses
    <a href="/licenses/new" class="btn btn-lg btn-success">New License</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>License number</th>
        <th>State</th>
        <th>Therapist</th>
        <th>Actions</th>
      </tr>

      <% @licenses.each do |license| %>
      <tr>
        <td><%= license.license_number %></td>
        <td><%= license.state_id %></td>
        <td><%= license.therapist_id %></td>
        <td>
          <a href="/licenses/<%= license.id %>" class="btn btn-primary">Show</a>
          <a href="/licenses/<%= license.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_license/<%= license.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
