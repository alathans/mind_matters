<div class="page-header">
  <h3>Time #<%= @time.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Time slot</dt>
      <dd><%= @time.time_slot %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/times" class="btn btn-primary">
        Back
      </a>
      <a href="/times/<%= @time.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_time/<%= @time.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
