<div class="page-header">
  <h3><%= @user.first_name %>'s Dashboard</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>Photo</dt>
      <dd><%= @user.photo %></dd>

      <dt>Address</dt>
      <dd><%= @user.address %></dd>

      <dt>Email</dt>
      <dd><%= @user.email %></dd>

      <dt>Password</dt>
      <dd><%= @user.password %></dd>

      <dt>Username</dt>
      <dd><%= @user.username %></dd>

      <dt>Last name</dt>
      <dd><%= @user.last_name %></dd>

      <dt>First name</dt>
      <dd><%= @user.first_name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/therapists" class="btn btn-primary">
        Back
      </a>
      <a href="/therapists/<%= @therapist.id %>/edit" class="btn btn-warning">
        Edit
      </a>
    </div>
  </div>
</div>
