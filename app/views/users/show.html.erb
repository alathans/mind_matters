<div class="page-header">
  <h3><%= current_user.first_name %>'s Dashboard</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>First name</dt>
      <dd><%= current_user.first_name %></dd>

      <dt>Last name</dt>
      <dd><%= current_user.last_name %></dd>

      <dt>Username</dt>
      <dd><%= current_user.username %></dd>

      <dt>Password</dt>
      <dd><%= current_user.password %></dd>

      <dt>Email</dt>
      <dd><%= current_user.email %></dd>

      <dt>Photo</dt>
      <dd><%= current_user.photo %></dd>

      <dt>Address</dt>
      <dd><%= current_user.address %></dd>






    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/users/<%= current_user.id %>/edit" class="btn btn-warning">
        Edit
      </a>
    </div>
  </div>
</div>
