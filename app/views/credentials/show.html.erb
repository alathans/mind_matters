<div class="page-header">
  <h3>Credential #<%= @credential.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @credential.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/credentials" class="btn btn-primary">
        Back
      </a>
      <a href="/credentials/<%= @credential.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_credential/<%= @credential.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
