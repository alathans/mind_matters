<div class="page-header">
  <h3>Therapist Note #<%= @therapist_note.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Body</dt>
      <dd><%= @therapist_note.body %></dd>

      <dt>Appointment</dt>
      <dd><%= @therapist_note.appointment_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/therapist_notes" class="btn btn-primary">
        Back
      </a>
      <a href="/therapist_notes/<%= @therapist_note.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_therapist_note/<%= @therapist_note.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
